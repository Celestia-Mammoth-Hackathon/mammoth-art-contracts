{
  "name": "@forma-dev/modularium-contracts",
  "description": "Smart contracts for Modularium",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/forma-dev/modularium-contracts.git"
  },
  "author": "mycodecrafting <josh@codecrafting.org>",
  "homepage": "https://forma.art",
  "files": [
    "/contracts/**/*.sol",
    "/abi"
  ],
  "dependencies": {
    "@forma-dev/sdk": "0.11.0",
    "@openzeppelin/contracts": "^5.0.0",
    "@openzeppelin/contracts-upgradeable": "^5.0.0",
    "dotenv": "^16.4.7",
    "pnpm": "^9.4.0",
    "web3": "^4.8.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "1",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-foundry": "^1.1.1",
    "@nomicfoundation/hardhat-ignition": "^0.15.2",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.2",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.10",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.6",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.8",
    "@openzeppelin/hardhat-upgrades": "^3.1.0",
    "@openzeppelin/merkle-tree": "^1.0.6",
    "@typechain/ethers-v5": "^11.1.2",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/node": "^20.12.12",
    "@wagmi/cli": "^2.1.7",
    "axios": "^1.7.2",
    "bignumber.js": "^9.1.2",
    "chai": "4",
    "dotenv-cli": "^7.4.2",
    "ethers": "^6.12.1",
    "hardhat": "^2.22.2",
    "hardhat-gas-reporter": "^2.1.1",
    "prettier": "^3.2.5",
    "prettier-plugin-solidity": "^1.3.1",
    "solhint": "^4.5.4",
    "solhint-community": "^3.7.0",
    "solhint-plugin-prettier": "^0.1.0",
    "solidity-coverage": "^0.8.12",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.8.0",
    "typechain": "^8.3.2",
    "typescript": "5.3.3"
  },
  "scripts": {
    "clean": "rm -fr abi artifacts cache out types",
    "lint": "solhint \"contracts/**/*.sol\"",
    "prettier": "prettier --write --plugin=prettier-plugin-solidity \"contracts/**/*.sol\"",
    "build": "forge build --extra-output-files bin --extra-output-files abi && pnpm hardhat compile",
    "build:ts": "pnpm hardhat-esm compile && tsc",
    "hardhat-esm": "NODE_OPTIONS='--experimental-loader ts-node/esm/transpile-only --no-warnings=ExperimentalWarning' hardhat --config hardhat.config.ts",
    "create-drop": "pnpm ts-node ./scripts/create-drop.ts",
    "mint-drop": "pnpm ts-node ./scripts/mint-drop.ts",
    "deploy:simple-drop": "pnpm ts-node ./scripts/deploy-simple-drop.ts",
    "deploy:simple-market": "pnpm ts-node ./scripts/deploy-simple-marketplace.ts",
    "deploy:simple-erc1155": "pnpm ts-node ./scripts/deploy-simple-erc1155.ts",
    "deploy:generative-erc721": "pnpm ts-node ./scripts/deploy-generative-erc721.ts",
    "test:generative-erc721": "pnpm ts-node ./scripts/test-generative-erc721.ts",
    "test:reveal-erc721": "pnpm ts-node ./scripts/test-reveal-erc721.ts",
    "upgrade:simple-erc1155": "pnpm ts-node ./scripts/upgrade-simple-erc1155.ts",
    "upgrade:simple-drop": "pnpm ts-node ./scripts/upgrade-simple-drop.ts",
    "upgrade:simple-market": "pnpm ts-node ./scripts/upgrade-simple-marketplace.ts",
    "testnet:create-drop": "pnpm dotenv -e .env.testnet -- pnpm create-drop",
    "testnet:mint-drop": "pnpm dotenv -e .env.testnet -- pnpm mint-drop",
    "testnet:deploy:simple-drop": "pnpm dotenv -e .env.testnet -- pnpm deploy:simple-drop",
    "testnet:deploy:simple-market": "pnpm dotenv -e .env.testnet -- pnpm deploy:simple-market",
    "testnet:deploy:simple-erc1155": "pnpm dotenv -e .env.testnet -- pnpm deploy:simple-erc1155",
    "testnet:deploy:generative-erc721": "pnpm dotenv -e .env.testnet -- pnpm deploy:generative-erc721",
    "testnet:test:generative-erc721": "pnpm dotenv -e .env.testnet -- pnpm test:generative-erc721",
    "testnet:test:reveal-erc721": "pnpm dotenv -e .env.testnet -- pnpm test:reveal-erc721",
    "testnet:upgrade:simple-erc1155": "pnpm dotenv -e .env.testnet -- pnpm upgrade:simple-erc1155",
    "testnet:upgrade:simple-drop": "pnpm dotenv -e .env.testnet -- pnpm upgrade:simple-drop",
    "testnet:upgrade:simple-market": "pnpm dotenv -e .env.testnet -- pnpm upgrade:simple-market",
    "testnet:verify": "pnpm dotenv -e .env.testnet -- pnpm hardhat verify",
    "mainnet:create-drop": "pnpm dotenv -e .env.mainnet -- pnpm create-drop",
    "mainnet:deploy:simple-drop": "pnpm dotenv -e .env.mainnet -- pnpm deploy:simple-drop",
    "mainnet:deploy:simple-market": "pnpm dotenv -e .env.mainnet -- pnpm deploy:simple-market",
    "mainnet:deploy:simple-erc1155": "pnpm dotenv -e .env.mainnet -- pnpm deploy:simple-erc1155",
    "mainnet:deploy:generative-erc721": "pnpm dotenv -e .env.mainnet -- pnpm deploy:generative-erc721",
    "mainnet:test:generative-erc721": "pnpm dotenv -e .env.mainnet -- pnpm test:generative-erc721",
    "mainnet:test:reveal-erc721": "pnpm dotenv -e .env.mainnet -- pnpm test:reveal-erc721",
    "mainnet:verify": "pnpm dotenv -e .env.mainnet -- pnpm hardhat verify"
  },
  "packageManager": "pnpm@9.1.1+sha512.14e915759c11f77eac07faba4d019c193ec8637229e62ec99eefb7cf3c3b75c64447882b7c485142451ee3a6b408059cdfb7b7fa0341b975f12d0f7629c71195"
}
